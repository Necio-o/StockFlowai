# ⚠️ RENOMBRA ESTE ARCHIVO A .env Y LLENA CON TUS VALORES
# NUNCA subas .env a Git - está en .gitignore

# Firebase Configuration
VITE_FIREBASE_API_KEY=tu_api_key_aqui
VITE_FIREBASE_AUTH_DOMAIN=tu_auth_domain_aqui
VITE_FIREBASE_PROJECT_ID=tu_project_id_aqui
VITE_FIREBASE_STORAGE_BUCKET=tu_storage_bucket_aqui
VITE_FIREBASE_MESSAGING_SENDER_ID=tu_messaging_sender_id_aqui
VITE_FIREBASE_APP_ID=tu_app_id_aqui
VITE_FIREBASE_MEASUREMENT_ID=tu_measurement_id_aqui

# Google Gemini API
VITE_GOOGLE_GEMINI_API_KEY=tu_gemini_api_key_aqui

# Optional: Email Service (para alertas)
VITE_EMAIL_SERVICE_API_KEY=tu_email_service_key_aqui
VITE_EMAIL_SERVICE_URL=https://api.mailgun.net/v3

# App Config
VITE_APP_ENV=production
VITE_MAX_FILE_SIZE_MB=10
VITE_MAX_PASSWORD_ATTEMPTS=5
VITE_LOCKOUT_TIME_MINUTES=15
